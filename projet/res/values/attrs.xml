<?xml version="1.0" encoding="utf-8"?>
<resources>
	<string name="app_name">Camera</string>

	<string name="logTag">AppLog</string>
	<string name="camTag">Camera</string>
	<string name="camListTag">CameraList</string>
	<string name="nbViewTag">nb</string>
	<string name="isWelcome">welcome</string>

	<string name="TimeOut">timeOutHTTP</string>
	<string name="defaultTimeOut">1000</string>

	<string name="limitFPS">limitFPS</string>
	<string name="defaultlimitFPS">500</string>

	<string name="NotifTO">NotifTO</string>
	<string name="defaultNotifTO">5000</string>

	<string name="SeuilDM">SeuilDM</string>
	<string name="defaultSeuilDM">30</string>


	<string name="sens">sens</string>
	<string name="defaultSens">10</string>
	<string name="maxSens">20</string>
	<string name="minSens">0</string>

	<string name="fileName">camera.ser</string>

	<string name="camPosition">Position</string>
	<string-array name="protocol_array">
		<item>http</item>
		<item>rtsp</item>
		<item>rtsphttp</item>
	</string-array>

	<string-array name="multi_view_array">
		<item>2</item>
		<item>3</item>
		<item>4</item>
		<item>5</item>
		<item>6</item>
	</string-array>

	<declare-styleable name="com.mnm.seekbarpreference.SeekBarPreference">
		<attr name="minValue" format="integer" />
		<attr name="maxValue" format="integer" />
	</declare-styleable>

	<color name="blue">#4FC3FF</color>
	<color name="white">#FFFFFF</color>
	<color name="black">#000000</color>
	<color name="silver">#C0C0C0</color>
	<color name="darkgray">#A9A9A9</color>
	<color name="greenyellow">#ADFF2F</color>
	<!-- Theme styles -->
	<style name="CustomTheme" parent="android:Theme.NoTitleBar">
		<item name="android:colorBackground">@color/black</item>
		<item name="android:buttonStyle">@style/StandardButton</item>
		<item name="android:textViewStyle">@style/StandardTextView</item>
		<item name="android:editTextStyle">@style/StandardEditText</item>
		<item name="android:listViewStyle">@style/StandardListView</item>
	</style>
	<!-- Button style -->
	<style name="StandardButton" parent="@android:style/Widget.Button">
		<item name="android:gravity">center_vertical|center_horizontal</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">-1</item>
		<item name="android:shadowRadius">0.2</item>
		<item name="android:textStyle">bold</item>
		<item name="android:background">@drawable/my_button_style</item>
		<item name="android:focusable">true</item>
		<item name="android:clickable">true</item>
	</style>
	<!-- TextView style -->
	<style name="StandardTextView" parent="@android:style/Widget.TextView">
		<item name="android:textColorHint">@color/greenyellow</item>
		<item name="android:textColor">@color/silver</item>
		<item name="android:typeface">monospace</item>
	</style>
	<!-- EditText style -->
	<style name="StandardEditText" parent="@android:style/Widget.EditText">
		<item name="android:background">@drawable/my_edit_text_style</item>
		<item name="android:textColorHint">@color/darkgray</item>
		<item name="android:textColor">@color/black</item>
	</style>
	<!-- ListText style -->
	<style name="StandardListView" parent="@android:style/Widget.ListView">
		<item name="android:cacheColorHint">#00000000</item>
		<item name="android:textColorHint">@color/greenyellow</item>
		<item name="android:textColor">@color/silver</item>
	</style>
	<!-- TextView List style -->
	<style name="StandardListItemInfo" parent="@android:style/Widget.TextView">
		<item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
		<item name="android:textColor">@color/silver</item>
		<item name="android:textStyle">italic</item>
	</style>
	<style name="StandardListItemHint" parent="@android:style/Widget.TextView">
		<item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
		<item name="android:textColor">@color/greenyellow</item>
	</style>

	<style name="theme_dialog" parent="android:style/Theme.Dialog">
		<item name="android:textColor">@color/silver</item>
		<item name="android:buttonStyle">@style/StandardButton</item>
		<item name="android:windowTitleStyle">@style/WindowTitleBackground</item>
	</style>
	<style name="WindowTitleBackground">
		<item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
		<item name="android:gravity">center</item>
		<item name="android:textColor">@color/greenyellow</item>
	</style>

</resources>