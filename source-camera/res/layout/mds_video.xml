<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/mds_video" android:layout_width="wrap_content"
	android:layout_height="wrap_content" android:layout_weight="1">
	<com.myapps.utils.drawRectOnTouchView
		android:id="@+id/drawRect" android:orientation="vertical"
		android:layout_height="fill_parent" android:layout_width="fill_parent" />
	<LinearLayout android:orientation="vertical"
		android:layout_height="fill_parent" android:layout_width="fill_parent">
		<Button android:layout_width="wrap_content" android:text="@string/run"
